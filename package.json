{
  "name": "cxa",
  "version": "0.0.2",
  "packageManager": "pnpm@6.32.3",
  "description": "all in one cli, to create app with any framework",
  "keywords": [
    "cli",
    "cxa",
    "create",
    "app",
    "framework",
    "react",
    "vue",
    "vite",
    "angular",
    "next",
    "nest"
  ],
  "homepage": "https://github.com/lawff/cxa#readme",
  "bugs": {
    "url": "https://github.com/lawff/cxa/issues"
  },
  "license": "MIT",
  "author": "lawliet",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lawff/cxa.git"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "bin",
    "dist"
  ],
  "bin": {
    "cxa": "./bin/cxa.mjs"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "scripts": {
    "build": "rimraf dist && unbuild",
    "lint": "eslint .",
    "release": "bumpp --commit --push --tag",
    "dev": "esno src/cli.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "execa": "^6.1.0",
    "kolorist": "^1.5.1",
    "minimist": "^1.2.6",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@antfu/ni": "^0.13.2",
    "@babel/types": "^7.17.0",
    "@lawlietff/eslint-config": "^0.0.9",
    "@types/minimist": "^1.2.2",
    "@types/node": "^17.0.23",
    "@types/prompts": "^2.0.14",
    "bumpp": "^7.1.1",
    "eslint": "^8.11.0",
    "esno": "^0.14.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.2",
    "unbuild": "^0.7.0",
    "vite": "^2.8.6",
    "vitest": "^0.7.10"
  }
}
